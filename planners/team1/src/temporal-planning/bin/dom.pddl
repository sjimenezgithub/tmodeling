( DEFINE ( DOMAIN DEPOT )
( :REQUIREMENTS :CONDITIONAL-EFFECTS :TYPING )
( :TYPES
	PLACE - OBJECT
	LOCATABLE - OBJECT
	DEPOT - PLACE
	DISTRIBUTOR - PLACE
	TRUCK - LOCATABLE
	HOIST - LOCATABLE
	SURFACE - LOCATABLE
	PALLET - SURFACE
	CRATE - SURFACE
)
( :PREDICATES
	( AT ?LOCATABLE0 - LOCATABLE ?PLACE1 - PLACE )
	( ON ?CRATE0 - CRATE ?SURFACE1 - SURFACE )
	( IN ?CRATE0 - CRATE ?TRUCK1 - TRUCK )
	( LIFTING ?HOIST0 - HOIST ?CRATE1 - CRATE )
	( AVAILABLE ?HOIST0 - HOIST )
	( CLEAR ?SURFACE0 - SURFACE )
	( EMPTYSTACK )
)
( :ACTION DO-DRIVE
  :PARAMETERS ( ?TRUCK0 - TRUCK ?PLACE1 - PLACE ?PLACE2 - PLACE )
  :PRECONDITION
	( AND
		( AT ?TRUCK0 ?PLACE1 )
		( EMPTYSTACK )
	)
  :EFFECT
	( AND
		( AT ?TRUCK0 ?PLACE2 )
		( NOT ( AT ?TRUCK0 ?PLACE1 ) )
	)
)
( :ACTION DO-LIFT
  :PARAMETERS ( ?HOIST0 - HOIST ?CRATE1 - CRATE ?SURFACE2 - SURFACE ?PLACE3 - PLACE )
  :PRECONDITION
	( AND
		( AVAILABLE ?HOIST0 )
		( AT ?CRATE1 ?PLACE3 )
		( ON ?CRATE1 ?SURFACE2 )
		( CLEAR ?CRATE1 )
		( AT ?HOIST0 ?PLACE3 )
		( EMPTYSTACK )
	)
  :EFFECT
	( AND
		( LIFTING ?HOIST0 ?CRATE1 )
		( CLEAR ?SURFACE2 )
		( NOT ( AT ?CRATE1 ?PLACE3 ) )
		( NOT ( CLEAR ?CRATE1 ) )
		( NOT ( AVAILABLE ?HOIST0 ) )
		( NOT ( ON ?CRATE1 ?SURFACE2 ) )
	)
)
( :ACTION DO-DROP
  :PARAMETERS ( ?HOIST0 - HOIST ?CRATE1 - CRATE ?SURFACE2 - SURFACE ?PLACE3 - PLACE )
  :PRECONDITION
	( AND
		( AT ?HOIST0 ?PLACE3 )
		( AT ?SURFACE2 ?PLACE3 )
		( CLEAR ?SURFACE2 )
		( LIFTING ?HOIST0 ?CRATE1 )
		( EMPTYSTACK )
	)
  :EFFECT
	( AND
		( AVAILABLE ?HOIST0 )
		( NOT ( LIFTING ?HOIST0 ?CRATE1 ) )
		( AT ?CRATE1 ?PLACE3 )
		( NOT ( CLEAR ?SURFACE2 ) )
		( CLEAR ?CRATE1 )
		( ON ?CRATE1 ?SURFACE2 )
	)
)
( :ACTION DO-LOAD
  :PARAMETERS ( ?HOIST0 - HOIST ?CRATE1 - CRATE ?TRUCK2 - TRUCK ?PLACE3 - PLACE )
  :PRECONDITION
	( AND
		( AT ?HOIST0 ?PLACE3 )
		( AT ?TRUCK2 ?PLACE3 )
		( LIFTING ?HOIST0 ?CRATE1 )
		( EMPTYSTACK )
	)
  :EFFECT
	( AND
		( NOT ( LIFTING ?HOIST0 ?CRATE1 ) )
		( IN ?CRATE1 ?TRUCK2 )
		( AVAILABLE ?HOIST0 )
	)
)
( :ACTION DO-UNLOAD
  :PARAMETERS ( ?HOIST0 - HOIST ?CRATE1 - CRATE ?TRUCK2 - TRUCK ?PLACE3 - PLACE )
  :PRECONDITION
	( AND
		( AVAILABLE ?HOIST0 )
		( IN ?CRATE1 ?TRUCK2 )
		( AT ?HOIST0 ?PLACE3 )
		( AT ?TRUCK2 ?PLACE3 )
		( EMPTYSTACK )
	)
  :EFFECT
	( AND
		( LIFTING ?HOIST0 ?CRATE1 )
		( NOT ( IN ?CRATE1 ?TRUCK2 ) )
		( NOT ( AVAILABLE ?HOIST0 ) )
	)
)
)
